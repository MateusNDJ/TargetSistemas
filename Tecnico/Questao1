#include <stdio.h>

int main() {
    // Inicializa as vari√°veis
    int INDICE = 13, SOMA = 0, K = 0;
    
    // Loop que soma os valores de 1 a 13
    while (K < INDICE) {
        K = K + 1;
        SOMA = SOMA + K;
    }
    
    // Imprime o resultado final
    printf("SOMA = %d\n", SOMA);
    return 0;
}
